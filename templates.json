{"FeatureChain":"<section class=\"ui-feature-chain\">\n    <div class=\"ui-container ui-feature-chain__container\">\n        {{# heading }}\n            <h2 class=\"ui-feature-chain__heading\">{{ . }}</h2>\n        {{/ heading }}\n        \n        {{# subheading }}\n            <h3 class=\"ui-feature-chain__subheading\">{{ . }}</h3>\n        {{/ subheading }}\n        \n        {{# body }}\n            <div class=\"ui-feature-chain__body\">{{ . }}</div>\n        {{/ body }}\n        \n        <div class=\"ui-feature-chain__items\">\n            {{# items }}\n                <div class=\"ui-feature-chain__item\">\n                    {{# image }} \n                        <div class=\"ui-feature-chain__item__image\">\n                            <img class=\"ui-feature-chain__item__image__source\" src=\"{{ contentUrl }}\" alt=\"{{ caption }}\">\n                        </div>\n                    {{/ image }}\n                    \n                    <div class=\"ui-feature-chain__item__details\">\n                        {{# heading }}\n                            <h3 class=\"ui-feature-chain__item__heading\">{{ . }}</h3>\n                        {{/ heading }}\n\n                        {{# body }}\n                            <div class=\"ui-feature-chain__item__body\">{{{ . }}}</div>\n                        {{/ body }}\n\n                        {{# link }}\n                            <a class=\"ui-feature-chain__item__link\" href=\"{{ href }}\" target=\"{{ target }}\">{{ text }}</a>\n                        {{/ link }}\n                    </div>\n                </div>\n            {{/ items }}\n            \n            {{# link }}\n                <a class=\"ui-feature-chain__link\" href=\"{{ href }}\" target=\"{{ target }}\">{{ text }}</a>\n            {{/ link }}\n        </div>\n    </div>\n</section>\n","FeatureRail":"<section class=\"ui-feature-rail {{# hasBackground }}-background{{/ hasBackground }}\" id=\"{{ identifier }}\">\n    <div class=\"ui-container ui-feature-rail__container\">\n        {{# heading }}\n            <h2 class=\"ui-feature-rail__heading\">{{ . }}</h2>\n        {{/ heading }}\n    </div>\n\n    <div class=\"ui-feature-rail__items\">\n        <div class=\"ui-feature-rail__items__track\">\n            {{# items }}\n                <div class=\"ui-feature-rail__item\">\n                    {{# image }}\n                        <img class=\"ui-feature-rail__item__image\" src=\"{{ contentUrl }}\" alt=\"{{ caption }}\">\n                    {{/ image }}\n\n                    {{# heading }}\n                        <h3 class=\"ui-feature-rail__item__heading\">{{ . }}</h3>\n                    {{/ heading }}\n\n                    {{# subheading }}\n                        <p class=\"ui-feature-rail__item__subheading\">{{ . }}</p>\n                    {{/ subheading }}\n\n                    {{# link }}\n                        <a target=\"{{ target }}\" href=\"{{ href }}\" class=\"ui-feature-rail__item__link\">{{ text }}</a>\n                    {{/ link }}\n                </div>\n            {{/ items }}\n        </div>\n    </div>\n</section>\n","Hero":"<div class=\"ui-hero\">\n    {{# image }}\n        <div class=\"ui-hero__media\">\n            <img class=\"ui-hero__media__source\" alt=\"{{ caption }}\" src=\"{{ contentUrl }}\">\n        </div>\n    {{/ image }}\n\n    <div class=\"ui-container ui-hero__container\">\n        {{# options.isHeader }}\n            {{# heading }}\n                <h1 class=\"ui-hero__heading\">{{ . }}</h1>\n            {{/ heading }}\n        {{/ options.isHeader }}\n        {{^ options.isHeader }}\n            {{# heading }}\n                <h2 class=\"ui-hero__heading\">{{ . }}</h2>\n            {{/ heading }}\n        {{/ options.isHeader }}\n        \n        {{# subheading }}\n            <h3 class=\"ui-hero__subheading\">{{ . }}</h3>\n        {{/ subheading }}\n\n        {{# body }}\n            <div class=\"ui-hero__body\">{{{ . }}}</div>\n        {{/ body }}\n\n        {{# link }}\n            <a class=\"ui-hero__link\" href=\"{{ href }}\" target=\"{{ target }}\">{{ text }}</a>\n        {{/ link }}\n    </div>\n</div>\n","Highlight":"<section class=\"ui-highlight {{# alignment }}-{{ . }}{{/ alignment }}\">\n    <div class=\"ui-container ui-highlight__container\">\n        <div class=\"ui-highlight__details\">\n            {{# heading }}\n                <h2 class=\"ui-highlight__heading\">{{ . }}</h2>\n            {{/ heading }}\n            \n            {{# subheading }}\n                <h3 class=\"ui-highlight__subheading\">{{ . }}</h3>\n            {{/ subheading }}\n\n            {{# body }}\n                <div class=\"ui-highlight__body\">{{{ . }}}</div>\n            {{/ body }}\n\n            {{# link }}\n                <div class=\"ui-highlight__link\">{{> Link }}</div>\n            {{/ link }}\n        </div>\n    \n        <div class=\"ui-highlight__media\">\n            {{# image }}\n                <img class=\"ui-highlight__media__source\" alt=\"{{ caption }}\" src=\"{{ contentUrl }}\">\n            {{/ image }}\n            \n            {{# video }}\n                <video controls class=\"ui-highlight__media__source\" title=\"{{ caption }}\" src=\"{{ contentUrl }}\" {{# image.contentUrl }}poster=\"{{ . }}\"{{/ image.contentUrl }}>\n            {{/ video }}\n        </div>\n    </div>\n</section>\n","MediaCollection":"<section class=\"ui-media-collection\">\n    <div class=\"ui-container ui-media-collection__container\">\n        <div class=\"ui-media-collection__text\">\n            {{# heading }}\n                <h2 class=\"ui-media-collection__heading\">{{ . }}</h2>\n            {{/ heading }}\n\n            {{# body }}\n                <div class=\"ui-media-collection__body\">{{{ . }}}</div>\n            {{/ body }}\n        </div>\n        <div class=\"ui-media-collection__items\">\n            <div class=\"ui-media-collection__items__track\">\n                {{# items }}\n                    <div for=\"ui-media-collection__item--{{ identifier }}\" class=\"ui-media-collection__item\">\n                        <label for=\"ui-media-collection__item--{{ identifier }}\" class=\"ui-media-collection__item__trigger\" title=\"{{ caption }}\" style=\"background-image:url('{{ thumbnailUrl }}');\"></label>\n                    </div>\n                {{/ items }}\n            </div>\n        </div>\n        <div class=\"ui-media-collection__display\">\n            {{# items }}\n                <input type=\"radio\" name=\"ui-media-collection\" id=\"ui-media-collection__item--{{ identifier }}\" class=\"ui-media-collection__item__source\">\n\n                {{# audio }}\n                    <audio src=\"{{ . }}\">\n                {{/ audio }}\n\n                {{# image }}\n                    <img src=\"{{ . }}\" alt=\"{{ caption }}\">\n                {{/ image }}\n                \n                {{# video }}\n                    <video src=\"{{ . }}\" controls>\n                {{/ video }}\n            {{/ items }}\n        </div>\n    </div>\n</section>\n"}